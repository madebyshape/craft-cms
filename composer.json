{
  "name": "madebyshape/craft-cms",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "4.10.1",
    "craftcms/cms": "5.8.18",
    "craftcms/mailgun": "^3.1.0",
    "nystudio107/craft-minify": "^5.0.0-beta.2",
    "nystudio107/craft-seomatic": "^5.0.0-beta.8",
    "nystudio107/craft-vite": "^5.0.0-beta.3",
    "putyourlightson/craft-blitz": "5.12.5",
    "putyourlightson/craft-sprig": "3.7.1",
    "servd/craft-asset-storage": "4.2.0",
    "spacecatninja/imager-x": "5.1.6",
    "verbb/formie": "3.1.4",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
