    "dependencies": {
        "alpinejs": "^2.3.5",
        "fg-loadcss": "^2.1.0",
        "js-cookie": "^2.2.1",
        "tailwindcss": "^1.4.6",
        "tailwindcss-animations": "^2.0.0",
        "tailwindcss-aspect-ratio": "^2.0.0",
        "tailwindcss-typography": "^3.1.0",
        "vanilla-lazyload": "^16.1.0"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.0",
        "browser-sync": "^2.26.7",
        "critical": "^1.3.9",
        "cssnano": "^4.1.10",
        "del": "^4.1.1",
        "favicons": "^5.5.0",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-dest-clean": "^0.5.0",
        "gulp-imagemin": "^5.0.3",
        "gulp-minify": "^3.1.0",
        "gulp-modernizr": "^3.4.1",
        "gulp-newer": "^1.4.0",
