{
    "name": "craftcms-tailwind",
    "version": "3.0.0",
    "description": "",
    "main": "gulpfile.js",
    "env": {
        "local": "http://craft-3.local"
    },
    "paths": {
        "dependencies": "node_modules/",
        "public": "public/",
        "templates": "templates/",
        "dist": {
            "base": "dist/",
            "css": "dist/css/",
            "js": "dist/js/",
            "images": "dist/images/",
            "favicon": "dist/images/favicon/"
        },
        "assets": {
            "base": "src/",
            "scss": "src/scss/",
            "js": "src/js/",
            "images": "src/images/",
            "fonts": "src/fonts/"
        }
    },
    "files": {
        "tailwind": "./tailwind.config.js",
        "dist": {
            "css": "app.css",
            "js": "app.js"
        },
        "assets": {
            "scss": "app.scss",
            "js": "index.js"
        }
    },
    "critical": {
        "dest": "_criticalCss/",
        "elements": [
            {
                "uri": "",
                "slug": "__home__"
            }
        ]
    },
    "purgeCss": {
        "whitelist": [],
        "whitelistPatterns": [
            "lazy-load.*",
            "flickity.*",
            "aos.*"
        ]
    },
    "cssDependencies": [],
    "dependencies": {
        "alpinejs": "^3.4.2",
        "fg-loadcss": "^3.1.0",
        "js-cookie": "^2.2.1",
        "tailwindcss-aspect-ratio": "^2.0.0",
        "tailwindcss-typography": "^3.1.0",
        "vanilla-lazyload": "^17.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@tailwindcss/jit": "^0.1.0",
        "@vue/compiler-sfc": "^3.0.7",
        "autoprefixer": "^10.0.2",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "browser-sync": "^2.26.14",
        "critical": "^2.0.6",
        "css-loader": "^5.1.3",
        "cssnano": "^4.1.10",
        "eslint": "^7.22.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "favicons": "^6.2.1",
        "file-loader": "^6.2.0",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-dest-clean": "^0.5.0",
        "gulp-imagemin": "^7.1.0",
        "gulp-minify": "^3.1.0",
        "gulp-newer": "^1.4.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^9.0.0",
        "gulp-purgecss": "^2.3.0",
        "gulp-rev": "^9.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sass-glob": "^1.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "mini-css-extract-plugin": "^1.3.9",
        "node-sass": "^7.0.0",
        "postcss": "^8.3.4",
        "purgecss": "^2.3.0",
        "rev-del": "^2.0.0",
        "tailwindcss": "^2.0.3",
        "vue": "^3.0.7",
        "vue-loader": "^16.1.2",
        "webpack": "^4.44.1",
        "webpack-merge": "^5.7.3",
        "webpack-stream": "^6.1.2"
    },
    "scripts": {
        "dev": "gulp dev",
        "prod": " NODE_ENV=prod gulp prod",
        "setup": "git pull && npm install && composer install && ./craft migrate/all --interactive=0 && ./craft project-config/apply --interactive=0 && gulp dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/madebyshape/craft-3.git"
    },
    "author": "Jason Mayo (@bymayo)",
    "authorUrl": "https://madebyshape.co.uk",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/madebyshape/craft-3/issues"
    },
    "homepage": "https://github.com/madebyshape/craft-3#readme"
}
